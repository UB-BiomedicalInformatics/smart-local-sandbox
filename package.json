{
  "name": "smart-sandbox",
  "version": "0.1.1",
  "description": "Docker based sandbox for smart apps",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smart-on-fhir/smart-local-sandbox.git"
  },
  "author": "vlad.ignatov@gmail.com",
  "bugs": {
    "url": "https://github.com/smart-on-fhir/smart-local-sandbox/issues"
  },
  "homepage": "https://github.com/smart-on-fhir/smart-local-sandbox/blob/master/README.md",
  "license": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:watch": "webpack -w --mode production",
    "build:watch:dev": "webpack -w --mode development",
    "start": "webpack-dev-server --open --mode production",
    "start:dev": "webpack-dev-server --open --mode development",
    "start:backend": "pm2 start backend/index.js",
    "pull:fhir-viewer": "   git subtree pull -P ext/fhir-viewer           --squash https://github.com/smart-on-fhir/fhir-viewer.git           master",
    "pull:sample-data": "   git subtree pull -P ext/generated-sample-data --squash https://github.com/smart-on-fhir/generated-sample-data.git master",
    "pull:xml-uploader": "  git subtree pull -P ext/xml-bundle-uploader   --squash https://github.com/smart-on-fhir/xml-bundle-uploader.git   master",
    "pull:smart-launcher": "git subtree pull -P ext/smart-launcher        --squash https://github.com/chb/smart-launcher.git                  master",
    "pull:tag-uploader": "  git subtree pull -P ext/tag-uploader          --squash https://github.com/smart-on-fhir/tag-uploader.git          master",
    "pull:synthea": "       git subtree pull -P ext/synthea               --squash https://github.com/synthetichealth/synthea.git             master",
    "pull:subtree": "npm run pull:fhir-viewer && npm run pull:sample-data && npm run pull:xml-uploader && npm run pull:smart-launcher && npm run pull:tag-uploader && npm run pull:synthea"
  },
  "keywords": [],
  "dependencies": {
    "colors": "^1.2.1",
    "commander": "^2.15.0",
    "express": "^4.16.2",
    "json5": "^1.0.1",
    "morgan": "^1.9.0",
    "pm2": "^2.10.1",
    "prop-types": "^15.6.1",
    "properties": "^1.2.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "request": "^2.85.0",
    "socket.io": "^2.0.4",
    "tree-kill": "^1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.10",
    "less": "^2.3.1",
    "less-loader": "^4.0.6",
    "style-loader": "^0.20.2",
    "webpack": "^4.0.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.9",
    "webpack-dev-server": "^3.1.0"
  }
}
